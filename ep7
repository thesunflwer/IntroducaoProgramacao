#include <stdio.h>

int main() {
  int valores[20], cont = 0, numero;
  
  for (int i = 0; i < 20; i++) {
    printf("Digite um numero para o array: ");
    scanf("%d", &valores[i]);
  }

  printf("Digite um numero repetido: ");
  scanf("%d", &numero);

  for (int i = 0; i < 20; i++) {
    if (numero == valores[i]){
      cont++;
    }
  }

  printf("O numero %d apareceu %d vez(es)", numero, cont);
}
